      <TextBlock Text="着"
               Padding="0"
               HorizontalAlignment="Center"
               VerticalAlignment="Top"
               Foreground="{StaticResource StaticTextColor}"
               FontFamily="../Fonts/JF-Dot-jiskan16s.ttf#JF Dot jiskan16s"
