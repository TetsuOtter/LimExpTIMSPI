                 Height="18"
               VerticalAlignment="Top"
               HorizontalAlignment="Left"
               Padding="0"
               Foreground="{Binding YBFlush2}"
               FontFamily="../Fonts/JF-Dot-jiskan16s.ttf#JF Dot jiskan16s"
               FontSize="16"/>
        <TextBlock x:Name="Joko1LimSP"
               Margin="369,8,0,30"
                 Width="27"
                 Height="18"
               VerticalAlignment="Top"
               HorizontalAlignment="Left"
               Padding="0"
               Foreground="{Binding YBFlush2}"
               FontFamily="../Fonts/JF-Dot-jiskan16s.ttf#JF Dot jiskan16s"
               FontSize="16"/>
        <TextBlock Text="km/h"
               Margin="396,8,0,30"
                 Width="36"
                 Height="18"
               VerticalAlignment="Top"
               HorizontalAlignment="Left"
               Padding="0"
               Foreground="{Binding YBFlush2}"
               FontFamily="../Fonts/JF-Dot-jiskan16s.ttf#JF Dot jiskan16s"
               FontSize="16"/>

      </Grid>
      <Grid x:Name="Joko2Grid">
        <TextBlock Text="km"
               Margin="135,30,0,8"
                 Width="18"
                 Height="18"
               VerticalAlignment="Top"
               HorizontalAlignment="Left"
               Padding="0"
               Foreground="White"
               FontFamily="../Fonts/JF-Dot-jiskan16s.ttf#JF Dot jiskan16s"
               FontSize="16"/>
